{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 0,
    "import/no-extraneous-dependencies": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-restricted-globals": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "import/prefer-default-export": 0
  },
  "plugins": ["jasmine"]
}
